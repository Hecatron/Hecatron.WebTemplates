{
  "name": "site-vue-bulma-tailwind-client",
  "description": "",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "yarn run build:dev",
    "build:dev": "yarn run build:dev:vendor && yarn run build:dev:site",
    "build:dev:site": "cross-env NODE_ENV=development webpack --config webpack/site.webpack.config.js --progress",
    "build:dev:vendor": "cross-env NODE_ENV=development webpack --config webpack/vendor.webpack.config.js --progress",
    "build:dev:watch": "cross-env NODE_ENV=development webpack --config webpack/site.webpack.config.js --progress --watch",
    "build:dev:server": "cross-env NODE_ENV=development webpack serve --config webpack/site.webpack.config.js",
    "build:prod": "yarn run build:prod:vendor && yarn run build:prod:site",
    "build:prod:site": "cross-env NODE_ENV=production webpack --config webpack/site.webpack.config.js --progress",
    "build:prod:vendor": "cross-env NODE_ENV=production webpack --config webpack/vendor.webpack.config.js --progress",
    "build:upgrade": "yarn upgrade-interactive",
    "build:install": "yarn install",
    "lint": "yarn run eslint . -f mo",
    "install": "yarn run build:dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "ajv": "^7.0.3",
    "buefy": "^0.9.4",
    "bulma": "^0.9.2",
    "event-source-polyfill": "^1.0.22",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.5.1",
    "metismenu": "^3.0.6",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-router": "^3.5.1",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.2.3",
    "axios": "^0.21.1",
    "bulmaswatch": "^0.8.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-formatter-mo": "^1.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "node-sass-package-importer": "^5.3.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-property-decorator": "^9.1.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.18.0",
    "webpack-cli": "4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0"
  }
